.head{width:750rpx;height: 180rpx;background:rgb(85, 211, 162);}
.headNum{display: flex; align-items: center; width: 100rpx;height: 50%; margin: 0 auto;color: white;}
.headWord{display: flex; align-items: center; width: 100rpx;height: 50%; margin: 0 auto;color: white;}

.tip{width:150rpx;height: 50rpx; margin-left:20rpx; margin-top: 10rpx;margin-bottom: 10rpx;
  box-shadow: 3px 3px 3px #ecebeb;
  border-radius: 9px;
}
.tip .word{margin-left: 15rpx;}
.task{
  display: flex;

  height:65px ;
  margin: 10px 10px;

  border:1px solid #BBBBBB;
  box-shadow: 5px 5px 5px #d4d2d2;
  border-radius: 9px;

  justify-content: space-between;
  align-items: center;
  background-color: #ffffff;
}
.task .taskName{margin-left: 50rpx;overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap; }
.task .icon-small-wrp{margin-right: 10rpx;}

.weui-slidecells{
  margin:8px;
}
.weui-slidecell{

  margin: 5px 5px;
  border:1px solid #BBBBBB;
  box-shadow: 0px 13px 5px #dbdada;
  border-radius: 15px;

  background-color:#FFFFFF;
  border-radius:8px;
  padding:24px;
  line-height:1.4;
  font-size:17px;
  color:rgba(0,0,0,.9);
  /*text-align:right;*/
}
.weui-slidecell__tips{
  color:rgba(0,0,0,.5);
}

.add{width: 80rpx;height: 80rpx;position: fixed;bottom: 70rpx;right:90rpx ;}
.gar{width: 80rpx;height: 80rpx;position: fixed;bottom: 210rpx;right:90rpx ;}
.hea{width: 80rpx;height: 80rpx;position: fixed;bottom: 350rpx;right:90rpx ;}